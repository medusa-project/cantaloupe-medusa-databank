#!/bin/bash
###
### boot - service hook for booting up services; called by
### /etc/init.d/services_startup
###

# Load environment variables
HOME=/home/iiif
. $HOME/.bashrc

# Mount S3 buckets
$HOME/bin/mount-buckets

# Start Cantaloupe
$HOME/bin/cantaloupe-medusa start
$HOME/bin/cantaloupe-databank start
